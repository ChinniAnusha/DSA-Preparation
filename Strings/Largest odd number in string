#Approach Traverse from the reverse and for each iteration check if the number is odd then from starting to until this point it is the largest odd number
#the above approach is only when the string doesnt start with zero
#source:Leetcode

class Solution:
    def largestOddNumber(self, num: str) -> str:
        for i in range(len(num)-1,-1,-1):
            if int(num[i]) % 2 == 1:
                return num[:i+1]
        return  "" # in case if there is no odd number 

#TC:O(n)
#SC:O(1)

#Approach:brute force check if starting element is zero or not and then proceed
def string(s):
    n = len(s)
    maxi = ""
    i = 0
    if s[i] == '0':
        for i in range(1,n):
           res = ""
           for j in range(i,n):
              if int(s[j]) % 2 == 1:
                   res = s[i:j+1]
                   if len(res) > len(maxi):
                        maxi = res
    else:
        for i in range(n):
           res = ""
           for j in range(i,n):
              if int(s[j]) % 2 == 1:
                   res = s[i:j+1]
                   if len(res) > len(maxi):
                        maxi = res
    
    return maxi
    
s = input()
print(string(s))

TC:O(n^3)
SC:O(n)


