#Approach:Bidirectional hashmap to store mapping from s to t and t to s
#source:Leetcode

class Solution:
    def isIsomorphic(self, s: str, t: str) -> bool:
        char_st = {}
        char_ts = {}

        for i in range(len(s)):
            char_s = s[i]
            char_t = t[i]
            if char_s in char_st:
                if char_st[char_s] != char_t:
                    return False
            else:
                char_st[char_s] = char_t
        
            if char_t in char_ts:
                if char_ts[char_t] != char_s:
                    return False
            else:
                char_ts[char_t] = char_s
        
    
        return True

#TC:O(n)
#SC:O(n)+O(n)
