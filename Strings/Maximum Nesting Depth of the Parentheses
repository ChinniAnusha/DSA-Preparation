#Approach:just use for loop no stack
#source:Leetcode

class Solution:
    def maxDepth(self, s: str) -> int:
        res = 0
        count = 0
        for ch in s:
            if ch == "(":
                count += 1
            elif ch == ")":
                count -= 1
            res = max(res,count)
        return res

#TC:O(n)
#SC:O(1)

