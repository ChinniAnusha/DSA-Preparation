#Approach:solved using for loops
#source:Leetcode
class Solution:
    def longestCommonPrefix(self, strs: List[str]) -> str:
          res = ""
          for i in range(len(strs[0])):
              for s in strs:
                  if i == len(s) or s[i] != strs[0][i]:
                        return res
              res += strs[0][i]
          return res

#TC:O(n*m)
#SC:O(m) depends on number of characters for longest common prefix
