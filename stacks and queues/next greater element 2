#brute force
class Solution:
    def nextGreaterElements(self, nums: List[int]) -> List[int]:
        n = len(nums)
        result = []
        for i in range(n):
             x = nums[i]
             found = False
             for j in range(i+1,i+n):
                  if nums[j%n] > x:
                      result.append(nums[j%n])
                      found = True
                      break
             if not found:
                result.append(-1)
             
        return result

#TC:O(n^2)
#SC:O(n)

#optimal
#solved using monotonic stack
class Solution:
    def nextGreaterElements(self, nums: List[int]) -> List[int]:
        n = len(nums)
        result = [-1]*n
        stack = []
        for i in range(2*n-1,-1,-1):
            ind = i%n
            current = nums[ind]
            while stack and stack[-1] <= current:
                stack.pop()
            if i < n:
                if stack:
                    result[i] = stack[-1]
            
            stack.append(current) 
             
        return result 
#TC:O(n)
#SC:O(n)
