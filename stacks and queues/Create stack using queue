from queue import Queue

class MyStack:

    def __init__(self):
        self.q = Queue()


    def push(self, x: int) -> None:
        s = self.q.qsize()
        self.q.put(x)

        for _ in range(s):
            self.q.put(self.q.get())


    def pop(self) -> int:
        n = self.q.queue[0]
        # Remove front element
        self.q.get()
        # Return removed element
        return n        

    def top(self) -> int:
        # Return front element
        return self.q.queue[0]

    def empty(self) -> bool:
         return self.q.empty()

#TC:O(n)
#SC:O(k)

#Implemented using two queues
from collections import deque

class MyStack:

    def __init__(self):
        self.q1 = deque()
        self.q2 = deque()

    def push(self, x: int) -> None:
        self.q2.append(x)
        while len(self.q1):
            self.q2.append(self.q1.popleft())
        
        self.q1,self.q2 = self.q2,self.q1 

    def pop(self) -> int:
        return self.q1.popleft()
             

    def top(self) -> int:
        return self.q1[0]
         
    def empty(self) -> bool:
         return not self.q1
#TC:O(1) for push and o(n) for both pop and top
#SC:O(n)
  
