class Solution:
    def isValid(self, s: str) -> bool:
        stack = []
        n = len(s)
        for i in range(n):
            if s[i] == '(' or s[i] == '[' or s[i] == '{':
                stack.append(s[i])
            else:
                if not stack:
                    return False
                ch = stack.pop()
                
                if (s[i] == ']' and ch == '[') or (s[i] == ')' and ch == '(') or (s[i] == '}' and ch == '{'):
                     continue
                else:
                    return False
        return not stack
#TC:O(n)
#SC:O(n)  
