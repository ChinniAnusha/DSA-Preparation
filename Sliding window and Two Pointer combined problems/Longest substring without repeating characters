#Optimal approach
class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:
        maxlen = 0
        l = 0
        r = 0
        n = len(s)
        hash = [-1] * 256
        while r < n:
            if hash[ord(s[r])] != -1:
                if hash[ord(s[r])] >= l:
                    l = hash[ord(s[r])] + 1
            length = r-l+1
            maxlen = max(length,maxlen)
            hash[ord(s[r])] = r
            r += 1 
    
        return maxlen


#TC:O(n)
#SC:O(256)
