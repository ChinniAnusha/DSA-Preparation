class ListNode:
    def __init__(self, val=0, next=None):
        self.val = val
        self.next = next

class Solution:
    def getLength(self, head: ListNode) -> int:
        count = 0
        curr = head

        while curr: 
            count += 1
            curr = curr.next

        return count

#TC:O(n)
#SC:O(1)
