#Brute force
# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def deleteMiddle(self, head: Optional[ListNode]) -> Optional[ListNode]:
         if head is None or head.next is None:
            return None
         count = 0
         temp = head
         while temp:
            count += 1
            temp = temp.next
         res = (count//2)
         temp = head
         while temp:
            res -= 1
            if res == 0:
                temp.next = temp.next.next
                break
            temp = temp.next
         return head
#TC:O(n)+O(n/2)
#SC:O(1)

#Optimal
# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def deleteMiddle(self, head: Optional[ListNode]) -> Optional[ListNode]:
         if head is None or head.next is None:
            return None
         slow = head
         fast = head
         fast = fast.next.next
         while fast != None and fast.next != None:
               slow = slow.next
               fast = fast.next.next
         
         slow.next = slow.next.next
         return head
#TC:O(n)
#SC:O(1)

