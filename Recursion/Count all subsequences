from typing import List

def findWays(arr: List[int], k: int) -> int:
       n = len(arr)
       def solve(i,summ,k):
           if i == len(arr):
               if summ == k:
                   return 1
               return 0
           summ += arr[i]
           l = solve(i+1,summ,k)
           summ -= arr[i]
           r = solve(i+1,summ,k)
           return l + r
       return solve(0,0,k) 
#TC:O(n*2^N)
#SC:O(n)
