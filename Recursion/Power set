#solved using bit manipulation
class Solution:
    def subsets(self, nums: List[int]) -> List[List[int]]:
         result = []
         n = len(nums)
         for i in range(0,2**n):
            subset = []
            for j in range(0,n):
                if i & (1 << j):
                    subset.append(nums[j])
            result.append(subset)
         return result
#TC:O(n*2^n)
#SC:O(n*2^n)
